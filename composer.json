{
    "name": "pfsense/pfsense",
    "require": {
        "mikehaertl/php-shellcommand": "^1.7",
        "symfony/cache": "^7.2",
        "nette/utils": "^4.0",
        "simplepie/simplepie": "^1.8",
        "symfony/console": "^7.2"
    },
    "autoload": {
    "psr-4": {
      "pfSense\\": "src/",
      "AI\\": "src/etc/inc/"
    },
    "files": [
      "src/etc/inc/ai.inc"
    ]
  },
    "config": {
        "vendor-dir": "src/usr/local/pfSense/include/vendor",
        "optimize-autoloader": true
    },
    "require-dev": {
        "rector/rector": "^0.15",
        "nikic/php-parser": "^4.15",
        "phpunit/phpunit": "^9.5"
    }
}
